pages = {222--248},
title = {{Evaluation of a simple, scalable, parallel best-first search strategy}},
url = {http://www.sciencedirect.com/science/article/pii/S0004370212001294$\backslash$nhttp://www.sciencedirect.com/science/article/pii/S0004370212001294\#$\backslash$nhttp://www.sciencedirect.com/science/article/pii/S0004370212001294/pdf?md5=19a1824f6f4b6532331da927313cde9b\&pid=1-s2.0},
