file = {:home/reyesamaro-a/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lardeux et al. - 2014 - Set Constraint Model and Automated Encoding into SAT Application to the Social Golfer Problem.pdf:pdf},
issn = {15729338},
keywords = {constraint programming,csp,encoding,sat,set constraints,social golfer problem},
title = {{Set Constraint Model and Automated Encoding into SAT: Application to the Social Golfer Problem}},
